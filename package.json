    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.3",
    "eslint-plugin-react-hooks": "^4.3.0",
    "gh-pages": "^2.2.0",
    "husky": "^8.0.1",
    "is-ci": "^3.0.1",
    "lint-staged": "^13.0.0",
    "near-cli": "^3.3.0",
    "near-seed-phrase": "^0.2.0",
    "postcss": "^8.4.16",
    "prettier": "^2.6.2",
    "react-styleguidist": "^11.2.0",
