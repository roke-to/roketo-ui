    "@near-wallet-selector/sender": "^6.0.0",
    "@near-wallet-selector/wallet-utils": "^6.0.0",
    "@roketo/sdk": "^0.2.9",
    "bignumber.js": "^9.0.2",
    "classnames": "^2.3.1",
    "clipboard-copy": "^4.0.1",
    "date-fns": "^2.29.3",
    "effector": "^22.3.0",
    "effector-react": "^22.1.4",
    "error-polyfill": "^0.1.3",
    "formik": "^2.2.9",
    "near-api-js": "^0.45.1",
    "numbro": "^2.3.5",
    "patronum": "^1.9.0",
    "rc-tooltip": "^5.1.1",
    "react": "^16.13.1",
    "react-datetime-picker": "^3.5.0",
    "react-dom": "^16.13.1",
    "react-modal": "^3.14.4",
    "react-router-dom": "^5.2.0",
    "swr": "^1.1.1",
    "type-fest": "^2.19.0",
    "vite-plugin-checker": "^0.4.6",
    "yup": "^0.32.9"
  },
  "devDependencies": {
    "@babel/core": "^7.18.2",
